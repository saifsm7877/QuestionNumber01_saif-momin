#include<iostream.h>
using namespace std;

int countDivisible(int n)
{
  int counter=0;
  if(n=0 || n<0)
  {
    cout<<"Please Provide a Valid positive integer";
   }
   else
   {
   for(int i=1;i<101;i++)
   {
     if(n%3==0)
      {
        counter++;
      }
    }
  return counter;
  }
  
 int main()
  {
  int n;
    cin>>n;
    cout<<countDivisible(n);
    return 0;
   } 
